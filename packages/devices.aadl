package devices
public
	with embeddedPlatform;

	device d_GPS
		features
			dp_GPSInOut: in out data port;
			busAccess: requires bus access embeddedPlatform::rs232_2;
		flows
			raw_data: flow source dp_GPSInOut;
		modes
			none;
		properties
			Period => 100 ms;
			Deadline => 100 ms;
			--Compute_Execution_Time => 20us .. 50us;

	end d_GPS;

	device implementation d_GPS.impl
	end d_GPS.impl;

